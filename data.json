{
  "expo": {
    "name": "Q Allure Login",
    "slug": "q-allure-login",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-white.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo-white.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-white.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/logo-transparent.png"
    }
  }
}